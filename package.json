{
  "name": "@zhid0399123/xcixtask",
  "version": "0.0.0",
  "description": "Modern and efficient RESTful to-do API built using the Fastify framework.",
  "main": "src/server.js",
  "directories": {
    "test": "__test__",
    "src": "src"
  },
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix",
    "pretest": "pnpm run lint",
    "test": "cross-env NODE_ENV=test tap --before=__test__/dockerode/run-before.js \"__test__/**/*.test.js\" --after=__test__/dockerode/run-after.js --no-check-coverage",
    "test:nostop": "cross-env NODE_ENV=test tap --before=__test__/dockerode/run-before.js \"__test__/**/*.test.js\" --no-check-coverage",
    "test:coverage": "cross-env NODE_ENV=test tap --coverage-report=html --before=__test__/dockerode/run-before.js \"__test__/**/*.test.js\" --after=__test__/dockerode/run-after.js --no-check-coverage",
    "start": "cross-env NODE_ENV=production fastify start --require src/config/tracing.js -l info --options src/server.js",
    "dev": "pnpm run mongo:start && fastify start --options -w -T 0 --debug src/server.js ",
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production') { require('husky').install() }\"",
    "version": "cz bump",
    "postversion": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "mongo:start": "docker run -d -p 27017:27017 --rm --name=xcixtask-mongo mongo:4",
    "mongo:stop": "docker container stop xcixtask-mongo",
    "docker:compose": "docker-compose -f docker-compose-test.yml up",
    "zipkin:start": "docker run --rm --name=xcixtask-zipkin -d -p 9411:9411 openzipkin/zipkin:2",
    "zipkin:stop": "docker container stop xcixtask-zipkin",
    "performance:assessment": "pnpm run dev && autocannon -d 20 -c 100 http://localhost:3000/v1/todos/",
    "clinic:doctor": "pnpm run dev && clinic doctor --on-port 'autocannon -d 20 -c 100 http://localhost:3000/v1/todos/' -- node clinic.js",
    "clinic:flame": "pnpm run dev && clinic flame --on-port 'autocannon -d 20 -c 100 http://localhost:3000/v1/todos/' -- node clinic.js",
    "clinic:heap": "pnpm run dev && clinic heap --on-port 'autocannon -d 20 -c 100 http://localhost:3000/v1/todos/' -- node clinic.js",
    "clinic:bubble": "pnpm run dev && clinic bubble --on-port 'autocannon -d 20 -c 100 http://localhost:3000/v1/todos/' -- node clinic.js"
  },
  "keywords": [
    "RESTful API",
    "to-do",
    "CRUD",
    "fastify",
    "nodejs"
  ],
  "author": "zhid0399123 <zhid0399123@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/cors": "^8.4.1",
    "@fastify/env": "^4.3.0",
    "@fastify/jwt": "^7.2.2",
    "@fastify/mongodb": "^8.0.0",
    "@fastify/multipart": "^8.0.0",
    "@fastify/sensible": "^5.0.0",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^1.10.1",
    "@opentelemetry/api": "1.3.0",
    "@opentelemetry/exporter-zipkin": "1.8.0",
    "@opentelemetry/instrumentation": "0.34.0",
    "@opentelemetry/instrumentation-dns": "0.31.0",
    "@opentelemetry/instrumentation-fastify": "0.31.0",
    "@opentelemetry/instrumentation-http": "0.34.0",
    "@opentelemetry/instrumentation-mongodb": "0.33.0",
    "@opentelemetry/sdk-node": "0.34.0",
    "@opentelemetry/sdk-trace-node": "1.8.0",
    "@opentelemetry/semantic-conventions": "1.8.0",
    "csv-parse": "^5.5.2",
    "csv-stringify": "^6.4.4",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.8.0",
    "fastify-metrics": "^10.3.3",
    "fastify-plugin": "^4.0.0",
    "pino-pretty": "^10.2.3"
  },
  "devDependencies": {
    "clinic": "12",
    "cross-env": "^7.0.3",
    "dockerode": "^4.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.2",
    "split2": "^4.2.0",
    "standard": "^17.1.0",
    "tap": "^16.1.0"
  }
}
